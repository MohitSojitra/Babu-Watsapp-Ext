{
    "manifest_version" : 2,
    "name" :"Babu D",
    "description" :"This extension shows a Google Image search result for the current page",
    "version" :"1.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["main.css"]
        }
        ],
    "page_action" :{
        "default_popup" :"popup.html",
        "default_title" : "watsapp contact"
    }

}